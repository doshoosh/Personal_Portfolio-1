

<%- include partials/header.ejs%>



<%- include partials/main_nav.ejs%>

  
  <div class="container row p-3 px-md-5">
  
    <div class="card text-white bg-info mb-3 col" style="max-width: 18rem;">
        <div class="card-header">Header</div>
        <div class="card-body">
            <h5 class="card-title">Info card title</h5>
            <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
        </div>
    </div>

    <div class="col-7">
    <!--the form-->

      <form>
        <div class="col">

        <div class="row">
          <div class="col">
            <label for="fname">First Name</label>
          </div>

          <div class="col-8">
            <input type="text" id="fname" name="firstname" placeholder="Your name..">
          </div>          
        </div>

        <div class="row">

          <div class="col">
            <label for="lname">Last Name</label>
          </div>
          <div class="col-8">
            <input type="text" id="lname" name="lastname" placeholder="Your last name..">
          </div>          
        </div>

        <div class="row">
          <div class="col">
            <label for="fname">Contact Number</label>
          </div>

          <div class="col-8">
            <input type="text" id="fname" name="firstname" placeholder="Your contact number..">
          </div>          
        </div>

        <div class="row">
          <div class="col">
            <label for="fname">Email Address</label>
          </div>

          <div class="col-8">
            <input type="text" id="fname" name="firstname" placeholder="Your email address..">
          </div>          
        </div>

        <div class="row">
          <div class="col">
            <label for="subject">Message</label>
          </div>
          <div class="col-8">
            <textarea id="subject" name="subject" placeholder="Write something.." style="height:170px"></textarea>
          </div>
          
        </div>
        
        

        <div class="row">
          <div class="col">
          </div>
          
          <div class="col-8">
            <input type="submit" value="Submit">
          </div>
          
        </div>
    
      </div>
      </form>

      
      

    </div>
  </div>
</div>

  
  

  
